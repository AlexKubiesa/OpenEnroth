    "runtime": {
        "version": "Lua 5.4",
        "path": ["./resources/scripts/?.lua"],
        "pathStrict": false
    },
    "workspace": {
        "maxPreload": 1600,
        "preloadFileSize": 1000,
        "ignoreDir": [
            "/locale/",
            "/libs/",
            "/3rd",
            "/.vscode",
            "/meta"
        ],
        "checkThirdParty": false
    },
    "typeFormat": {
        "config": {
            "format_line": "false"
        }
    },
    "type": {
        "castNumberToInteger": false
    },
    "doc": {
        "privateName": [
            "_*"
        ]
    }
